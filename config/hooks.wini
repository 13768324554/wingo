[ForceFloating]
match := MatchClientClass ":client:" "gimp"

managed := ToggleFloating ":client:"

[FocusTransparency]
match := True

focused := SetOpacity ":client:" 1.0
unfocused := SetOpacity ":client:" 0.8

[Dale]
match := True

startup := Dale

[Autostart]
match := True

startup := Shell "chromium"
startup := Shell "gkrellm"
startup := Shell "pager-multihead"

